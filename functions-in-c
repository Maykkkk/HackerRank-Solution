#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int main() {
    int a, b, c, d, ans;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    ans = a;
    if(ans < b) {
        ans = b;
    }
    if(ans < c) {
        ans = c;
    }
    if(ans < d) {
        ans = d;
    }
    printf("%d",ans);
    return 0;
}
